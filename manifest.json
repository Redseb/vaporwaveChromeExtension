{
    "manifest_version": 3,
    "name": "generate vaporwave",
    "version": "1.3",
    "permissions": [
      "storage",
      "contextMenus",
      "clipboardWrite"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "generate vaporwave"
    },
    "background": {
      "scripts": ["eventPage.js"],
      "persistent": false
    }
  }