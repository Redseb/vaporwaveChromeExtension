{
  "manifest_version": 2,
  "name": "Vaporwave Aesthetic Generator",
  "version": "0.0.1",
  "permissions": [
    "storage",
    "contextMenus",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Vaporwave Aesthetic Generator"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}